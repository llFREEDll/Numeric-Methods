<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="M04.M04Controller">
    <stylesheets>
        <URL value="@m04.css" />
    </stylesheets>
   <children>
      <TableView fx:id="table" layoutX="-2.0" layoutY="200.0" prefHeight="200.0" prefWidth="600.0">
        <columns>
          <TableColumn prefWidth="75.0" text="x" fx:id="x" />
          <TableColumn fx:id="y" prefWidth="75.0" text="y" />
            <TableColumn prefWidth="75.0" text="x^2" fx:id="xx" />
            <TableColumn prefWidth="75.0" text="x^3" fx:id="xxx" />
            <TableColumn prefWidth="75.0" text="x^4" fx:id="xxxx" />
            <TableColumn prefWidth="75.0" text="x^2*y" fx:id="x2y" />
            <TableColumn prefWidth="75.0" text="xy" fx:id="xy" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <TextField fx:id="tfx" layoutX="51.0" layoutY="19.0" prefHeight="16.0" prefWidth="38.0" />
      <TextField fx:id="tfy" layoutX="152.0" layoutY="19.0" prefHeight="25.0" prefWidth="38.0" />
      <Label layoutX="14.0" layoutY="18.0" text="x=" />
      <Label layoutX="114.0" layoutY="18.0" text="y=" />
      <Button fx:id="add" layoutX="215.0" layoutY="11.0" mnemonicParsing="false" onAction="#add" text="add" />
      <Button layoutX="300.0" layoutY="11.0" mnemonicParsing="false" onAction="#Evaluate" text="Evalute" />
      <Label fx:id="show" layoutX="23.0" layoutY="121.0" text="f(x)=?" />
      <Button layoutX="516.0" layoutY="11.0" mnemonicParsing="false" onAction="#GoBack" text="Back" />
   </children>
</AnchorPane>
